{
  "name": "boatable-social",
  "version": "1.0.0",
  "description": "Complete maritime social network and vessel management platform",
  "private": true,
  "license": "MIT",
  "keywords": [
    "boating",
    "maritime",
    "social-network",
    "vessel-management",
    "marine",
    "boat-maintenance",
    "weather",
    "navigation",
    "marketplace",
    "crew-management"
  ],
  "author": "Boatable Development Team",
  "homepage": "https://boatable.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/boatable/boatable-social.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "dev:local": "cross-env NEXT_PUBLIC_USE_EMULATOR=true next dev",
    "test": "jest",
    "test:e2e": "testcafe edge src/tests/",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "generate-data": "bun src/scripts/dataGeneration/generateBoatableData.ts",
    "emulators": "firebase emulators:start --import=./firebase-export --export-on-exit",
    "deploy": "./deploy.sh",
    "clear-cache": "next build --debug",
    "migrate": "echo 'Running database migrations...' && npm run generate-data"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@faker-js/faker": "^9.9.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@mapbox/mapbox-gl-geocoder": "^5.0.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@mui/x-date-pickers": "^7.29.2",
    "@react-google-maps/api": "^2.19.3",
    "@reduxjs/toolkit": "^2.8.0",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "firebase": "^12.1.0",
    "framer-motion": "^11.0.0",
    "mapbox-gl": "^3.1.0",
    "next": "^14.2.28",
    "ora": "^8.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.0",
    "react-redux": "^9.2.0",
    "recharts": "^2.12.0",
    "swr": "^2.2.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "testcafe": "^0.16.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5"
  }
}
